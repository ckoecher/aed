        <activity
            android:name="vco.aed.app.AddPrevious"
            android:label="@string/title_activity_add_previous" >
        </activity>
        <activity
            android:name="vco.aed.app.EventDetails"
            android:label="@string/title_activity_event_details" >
        </activity>
        <activity
            android:name="vco.aed.app.NewEvent"
            android:label="@string/title_activity_new_event" >
